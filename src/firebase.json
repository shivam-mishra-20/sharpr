{
  "hosting": {
    "headers": [
      {
        "source": "/admin_dashboard/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/parent_dashboard/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}